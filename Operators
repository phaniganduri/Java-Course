//Relational Operator

package com.relational;
public class Relational {
	public static void main(String[] args) {
		
		int a = 10;
		int b =  20;
		
		boolean result = a > b;
		System.out.println(result);
		
		result = a < b;
		System.out.println(result);
		
		result = (a == b);
		System.out.println(result);
		
		result = (a != b);
		System.out.println(result);	
		
	}

}


//Unary operator // Increment
package com.relational;
public class Unary {
	public static void main(String[] args) {
		int a = 10;
		System.out.println(a++);
		System.out.println(a);
		
		int b = 20;
		b++;
		System.out.println(++b);		
		
	}

}

//Unary operator // Decrement

package com.relational;
public class UnaryDecrement {
	public static void main(String[] args) {
		 int a = 12;
		 a--;
		 System.out.println(a++);
		 
		 int b = 22;
		 
		 --b;
		 System.out.println(++b);
	}

}





