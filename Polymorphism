package com.poly;
public class MethodOver {
	public void show() {
		System.out.println("Show without param");
	}
	public void show(int a) {
		System.out.println("Show with int param");
	}
	public void show(String a) {
		System.out.println("Show with String param");
	}
	public void show(int a,String b) {
		System.out.println("Show with int,String param");
	}
	public void show(String a,int b) {
		System.out.println("Show with String,int param");
	}

}
//Test Class
package com.poly;
public class MethodOverTest {
	public static void main(String[] args) {
		MethodOver over = new MethodOver();
		over.show();
		over.show(10);
		over.show("Phani");
		over.show(10,"Anand");
		over.show("ABC", 230);
		
	}

}
